<div class="row">
  <div class="col-xs-offset-3 col-xs-6">
    <div class="panel panel-default">
      <div class="panel-heading">アカウント情報</div>
      <div class="panel-body">
        <%= form_for(:link, url: setting_update_path) do |f| %>
            <div class="form-group">
              <%= f.label :github, 'GitHubのURL' %>
              <%= text_field :github, :link, class: 'form-control' %>
            </div>

            <div class="form-group">
              <%= f.label :cloud9, 'cloud9のURL' %>
              <%= text_field :cloud9, :link, class: 'form-control' %>
            </div>

            <div class="text-right">
              <%= f.submit '変更する', class: 'btn btn-success' %>
            </div>
        <% end %>
      </div>
    </div>
  </div>
</div>